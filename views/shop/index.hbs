<div class="row">
  {{# each products}}
      <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
          <img src="{{this.imagePath}}" alt="..." class="img-responsive">
          <div class="caption">
            <h3>{{this.title}}</h3>
            <p class="description">{{this.description}}</p>
            <div class="clearfix">
              <div class="price pull-left">${{this.price}}</div>&nbsp;<span class="label label-info">Free shipping</span>
              <a href="/cart/{{this.id}}" class="btn btn-success pull-right" role="button">Add to Cart</a>
            </div>
          </div>
        </div>
      </div>
  {{/each}}
</div>